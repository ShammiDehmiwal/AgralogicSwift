# Uncomment the next line to define a global platform for your project
 platform :ios, '10.2'

target 'AgraLogicSwift' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for AgraLogicSwift
 
 pod 'ReachabilitySwift'  # for network check connectivity.
  pod 'Alamofire', :git => 'https://github.com/Alamofire/Alamofire.git', :tag => '4.7.0'
  pod 'IQKeyboardManagerSwift' #for auto keyboard manage.

  


  target 'AgraLogicSwiftTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AgraLogicSwiftUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
